{
  "name": "@blunote/backend",
  "version": "2.0.0",
  "private": true,
  "description": "BluNote LTI backend service with AgentKit runtime",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit",
    "db:migrate": "tsx src/data/migrate.ts"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@openai/agents": "^0.1.9",
    "@supabase/supabase-js": "^2.75.0",
    "dotenv": "^16.4.1",
    "fastify": "^4.25.2",
    "jose": "^5.2.0",
    "openai": "^6.3.0",
    "pino-pretty": "^13.1.2",
    "ws": "^8.16.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vitest": "^1.2.1"
  }
}
